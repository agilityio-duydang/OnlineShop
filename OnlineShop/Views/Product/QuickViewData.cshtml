                        @if (productPictures.Count > 0)
                        {
                            var url = new Models.Dao.PictureDao().GetPictureUrl(productPictures.SingleOrDefault().Id, 600, true, null, Models.Dao.PictureType.Entity);
                            <div class="picture">
                                <a>
                                    <img class="cloudzoom" src="@url" data-cloudzoom="appendSelector: '.quickViewWindow .gallery .picture', zoomPosition: 'inside', zoomOffsetX: 0, easing: 3, zoomFlyOut: false, disableZoom: 'auto'" alt="Picture of Droid Inventor Kit" id="quick-view-zoom" style="user-select: none;">
                                </a>
                            </div>
                        }
                        else
                        {
                            var url = new Models.Dao.PictureDao().GetDefaultPictureUrl(600, Models.Dao.PictureType.Entity, null);
                            <div class="picture">
                                <a>
                                    <img class="cloudzoom" src="@url" data-cloudzoom="appendSelector: '.quickViewWindow .gallery .picture', zoomPosition: 'inside', zoomOffsetX: 0, easing: 3, zoomFlyOut: false, disableZoom: 'auto'" alt="Picture of Droid Inventor Kit" id="quick-view-zoom" style="user-select: none;">
                                </a>
                            </div>
                        }
                        <div class="thumbnails">
                            @foreach (var item in productPictures)
                            {
                                var url = new Models.Dao.PictureDao().GetPictureUrl(item.Id, 600, true, null, Models.Dao.PictureType.Entity);
